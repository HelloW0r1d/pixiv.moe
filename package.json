{
  "name": "pixiv.moe",
  "version": "1.1.14",
  "description": "Source code of https://pixiv.moe",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/CNAME ./dist",
    "predist": "npm run clean && npm run copy",
    "dist": "cross-env NODE_ENV=production webpack --progress --config build/webpack.config.js",
    "postdist": "copyfiles -f ./src/icons/*.ico ./src/icons/*.png ./dist/assets",
    "lint": "eslint --max-warnings 0 ./src",
    "posttest": "npm run lint",
    "start": "cross-env NODE_ENV=development node build/dev-server.js",
    "test": "cross-env NODE_ENV=test istanbul cover karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "ci-publish": "ci-publish",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "check": "yarn upgrade-interactive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe.git"
  },
  "keywords": [
    "pixiv",
    "lovelive"
  ],
  "author": "kokororin",
  "license": "MIT",
  "homepage": "https://github.com/LoveLiveSunshine/pixiv.moe#readme",
  "bugs": {
    "url": "https://github.com/LoveLiveSunshine/pixiv.moe/issues"
  },
